{
  "name": "GoogleCloudStorage",
  "type": "executer",
  "exposeRoute": false,
  "execProgram": "node",
  "main": "app.js",
  "version": "1.1.2",
  "imgUrl": "logo.png",
  "category": ["CLOUD", "STORAGE"],
  "settings": [
    {
      "name": "CREDENTIALS",
      "viewName": "Service Account Credentials",
      "type": "vault",
      "description": "Default service account credentials",
      "learnUrl": "https://cloud.google.com/docs/authentication/production"
    },
    {
      "name": "PROJECT",
      "viewName": "Default Project ID",
      "type": "string",
      "description": "The ID of the default project to send requests to.",
      "placeholder": "my-project",
      "learnUrl": "https://cloud.google.com/resource-manager/docs/creating-managing-projects"
    }
  ],
  "methods": [
    {
      "name": "CREATE_BUCKET",
      "viewName": "Create New Bucket",
      "description": "To create storage bucket.",
      "learnUrl": "https://cloud.google.com/storage/docs/creating-buckets",
      "params": [
        {
          "name": "PROJECT",
          "viewName": "Project ID",
          "type": "string",
          "description": "The ID of the default project to send requests to.",
          "placeholder": "my-project",
          "learnUrl": "https://cloud.google.com/docs/authentication/production"
        },
        {
          "name": "CREDENTIALS",
          "viewName": "Credentials",
          "type": "vault",
          "description": "Default service account credentials",
          "learnUrl": "https://cloud.google.com/docs/authentication/production"
        },
        {
          "name": "NAME",
          "viewName": "Bucket Name",
          "type": "string",
          "description": "The name of the bucket",
          "placeholder": "my-bucket",
          "learnUrl": "https://cloud.google.com/storage/docs/creating-buckets"
        },  
        {
          "name": "CLASS",
          "viewName": "Storage Class",
          "type": "options",
          "description": "The name of the bucket",
          "placeholder": "my-bucket",
          "learnUrl": "https://cloud.google.com/storage/docs/creating-buckets",
          "options": [
            {
              "id": "STANDARD",
              "name": "Standard"
            },
            {
              "id": "NEARLINE",
              "name": "Nearline"
            },
            {
              "id": "COLDLINE",
              "name": "Coldline"
            },
            {
              "id": "ARCHIVE",
              "name": "Archive"
            }
          ]
        },
        {
          "name": "LOCATION",
          "viewName": "Location",
          "type": "string",
          "description": "Cloud Loaction",
          "placeholder": "us-west2",
          "learnUrl": "https://cloud.google.com/about/locations"
        }
      ]
    },
    {
      "name": "DELETE_BUCKET",
      "viewName": "Delete Bucket",
      "description": "To delete storage bucket.",
      "learnUrl": "https://cloud.google.com/storage/docs/deleting-buckets",
      "params": [
        {
          "name": "PROJECT",
          "viewName": "Project ID",
          "type": "string",
          "description": "The ID of the default project to send requests to.",
          "placeholder": "my-project",
          "learnUrl": "https://cloud.google.com/docs/authentication/production"
        },
        {
          "name": "CREDENTIALS",
          "viewName": "Credentials",
          "type": "vault",
          "description": "Default service account credentials",
          "learnUrl": "https://cloud.google.com/docs/authentication/production"
        },
        {
          "name": "NAME",
          "viewName": "Bucket Name",
          "type": "autocomplete",
          "autocompleteType": "function",
          "functionName": "listBuckets",
          "description": "The name of the bucket",
          "placeholder": "my-bucket",
          "learnUrl": "https://cloud.google.com/storage/docs/deleting-buckets"
        }
      ]
    },
    {
      "name": "UPLOAD_FILE",
      "viewName": "Upload File",
      "description": "To upload file.",
      "learnUrl": "https://cloud.google.com/storage/docs/uploading-objects",
      "params": [
        {
          "name": "PROJECT",
          "viewName": "Project ID",
          "type": "string",
          "description": "The ID of the default project to send requests to.",
          "placeholder": "my-project",
          "learnUrl": "https://cloud.google.com/docs/authentication/production"
        },
        {
          "name": "CREDENTIALS",
          "viewName": "Credentials",
          "type": "vault",
          "description": "Default service account credentials",
          "learnUrl": "https://cloud.google.com/docs/authentication/production"
        },
        {
          "name": "NAME",
          "viewName": "Bucket Name",
          "type": "autocomplete",
          "autocompleteType": "function",
          "functionName": "listBuckets",
          "description": "The name of the bucket",
          "placeholder": "my-bucket",
          "learnUrl": "https://cloud.google.com/storage/docs/creating-buckets"
        },
        {
          "name": "FILE_PATH",
          "viewName": "File Path",
          "type": "string",
          "description": "File Location/Path",
          "placeholder": "d:/myfile",
          "learnUrl": "https://cloud.google.com/storage/docs/uploading-objects"
        }
      ]
    },
    {
      "name": "DELETE_FILE",
      "viewName": "Delete File",
      "description": "To upload file.",
      "learnUrl": "https://cloud.google.com/storage/docs/samples/storage-delete-file",
      "params": [
        {
          "name": "PROJECT",
          "viewName": "Project ID",
          "type": "string",
          "description": "The ID of the default project to send requests to.",
          "placeholder": "my-project",
          "learnUrl": "https://cloud.google.com/docs/authentication/production"
        },
        {
          "name": "CREDENTIALS",
          "viewName": "Credentials",
          "type": "vault",
          "description": "Default service account credentials",
          "learnUrl": "https://cloud.google.com/docs/authentication/production"
        },
        {
          "name": "NAME",
          "viewName": "Bucket Name",
          "type": "autocomplete",
          "autocompleteType": "function",
          "functionName": "listBuckets",
          "description": "The name of the bucket",
          "placeholder": "my-bucket",
          "learnUrl": "https://cloud.google.com/storage/docs/creating-buckets"
        },
        {
          "name": "FILE_NAME",
          "viewName": "File Name",
          "type": "autocomplete",
          "autocompleteType": "function",
          "functionName": "listFiles",
          "learnUrl": "https://cloud.google.com/storage/docs/samples/storage-delete-file"
        }
      ]
    },
    {
      "name": "CREATE_FOLDER",
      "viewName": "Upload File into Folder",
      "description": "To Create file.",
      "learnUrl": "https://cloud.google.com/storage/docs/uploading-objects",
      "params": [
        {
          "name": "PROJECT",
          "viewName": "Project ID",
          "type": "string",
          "description": "The ID of the default project to send requests to.",
          "placeholder": "my-project",
          "learnUrl": "https://cloud.google.com/docs/authentication/production"
        },
        {
          "name": "CREDENTIALS",
          "viewName": "Credentials",
          "type": "vault",
          "description": "Default service account credentials",
          "learnUrl": "https://cloud.google.com/docs/authentication/production"
        },
        {
          "name": "NAME",
          "viewName": "Bucket Name",
          "type": "autocomplete",
          "autocompleteType": "function",
          "functionName": "listBuckets",
          "description": "The name of the bucket",
          "placeholder": "my-bucket",
          "learnUrl": "https://cloud.google.com/storage/docs/creating-buckets"
        },
        {
          "name": "FOLDER_NAME",
          "viewName": "Folder Name",
          "type": "string",
          "description": "Folder name where the file will be stored into the given bucket",
          "placeholder": "my-Folder",
          "learnUrl": "https://cloud.google.com/storage/docs/uploading-objects"
        },
        {
          "name": "File_NAME",
          "viewName": "File Name",
          "type": "string",
          "description": "File name with extension that is to be uploaded",
          "placeholder": "my-File.txt",
          "learnUrl": "https://cloud.google.com/storage/docs/uploading-objects"
        },
        {
          "name": "FILE_PATH",
          "viewName": "File Path",
          "type": "string",
          "description": "File Location/Path",
          "placeholder": "d:/myfile.txt",
          "learnUrl": "https://cloud.google.com/storage/docs/uploading-objects"
        }

      ]
    }
  ]
}
